{
    "$schema": "http://turborepo.org/schemas.json",
    "pipeline": {
        "dev" : {
            "cache": false
        },
        "build" : {
            "outputs": [
                "dist/**",
                "storybook-static/**"
            ],
            "dependsOn": [
                "^build"
            ]
        },
        "lint" : {
            "dependsOn": [
                "^lint"
            ]
        }
    }
}